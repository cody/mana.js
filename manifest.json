{
	"manifest_version": 2,
	"name": "The Mana World",
	"short_name": "Mana.js",
	"description": "A free open source 2d MMORP Game",
	"version": "2013.11.12",
	"app": {
		"background": {
			"scripts": ["background.js"]
		}
	},
	"icons": {
		"16": "graphics/icon16.png",
		"48": "graphics/icon48.png",
		"128": "graphics/icon128.png"
	},
	"permissions": [
		{"socket": ["tcp-connect"]},
		"unlimitedStorage",
		"<all_urls>"
	]
}
