{
	"manifest_version": 2,
	"name": "The Mana World",
	"short_name": "TMW",
	"description": "A free open source 2d MMORPG",
	"version": "2013.11.29",
	"minimum_chrome_version": "29",
	"app": {
		"background": {
			"scripts": ["background.js"]
		}
	},
	"icons": {
		"16": "graphics/icon16.png",
		"48": "graphics/icon48.png",
		"128": "graphics/icon128.png"
	},
	"permissions": [
		{"socket": ["tcp-connect"]},
		"unlimitedStorage",
		"<all_urls>",
		{"fileSystem": ["directory", "retainEntries"]},
		"storage"
	]
}
